{
  "name": "test_auto_load",
  "description": "测试自动加载功能的指令",
  "template": "test {param1} {param2}",
  "parameters": {
    "param1": {
      "name": "param1",
      "description": "测试参数1",
      "type": "string",
      "required": true,
      "defaultValue": "hello"
    },
    "param2": {
      "name": "param2",
      "description": "测试参数2",
      "type": "enum",
      "required": false,
      "defaultValue": "option1",
      "options": ["option1", "option2", "option3"],
      "optionLabels": {
        "option1": "选项1",
        "option2": "选项2",
        "option3": "选项3"
      }
    }
  }
}